{
  "name": "dkit",
  "version": "0.1.4",
  "description": "React design kit components",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "styles.css",
    "tailwind-preset.cjs"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./tailwind-preset": {
      "require": "./tailwind-preset.cjs"
    },
    "./styles.css": "./styles.css"
  },
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "build:css": "npx tailwindcss -i ./src/styles.css -o ./styles.css --minify",
    "build": "npm run build:css && tsup src/index.ts --format cjs,esm --dts",
    "dev": "npm run build:css && tsup src/index.ts --format cjs,esm --dts --watch",
    "prepare": "npm run build"
  },
  "peerDependencies": {
    "react": "^18",
    "react-dom": "^18"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.2.1",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "autoprefixer": "^10.4.27",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.2.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
}
